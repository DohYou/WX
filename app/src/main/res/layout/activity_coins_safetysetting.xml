<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_marginBottom="60dp"
    android:orientation="vertical"
    android:background="@color/grayF5F5F5">
    <include
        layout="@layout/title_layout"/>
    <com.ylr.hyy.utils.ItemViewGroup
        android:layout_width="match_parent"
        android:layout_height="51dp"
        android:id="@+id/me_coins_safety_setting1"
        android:background="@color/all_activity_FFFFFF"
        android:layout_marginLeft="-20dp"/>
    <View
        android:layout_width="match_parent"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_height="0.5dp"
        android:background="@color/all_line_bg"/>
    <com.ylr.hyy.utils.ItemViewGroup
        android:layout_width="match_parent"
        android:layout_height="51dp"
        android:id="@+id/me_coins_safety_setting2"
        android:background="#FFFFFF"
        android:layout_marginLeft="-20dp"/>
    <View
        android:layout_width="match_parent"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_height="0.5dp"
        android:background="@color/all_line_bg"/>
    <com.ylr.hyy.utils.ItemViewGroup
        android:layout_width="match_parent"
        android:layout_height="51dp"
        android:id="@+id/me_coins_safety_setting3"
        android:background="#FFFFFF"
        android:layout_marginLeft="-20dp"/>
</LinearLayout>